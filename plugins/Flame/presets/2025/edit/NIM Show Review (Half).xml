<?xml version="1.0"?>
<preset version="14">
        <type>movie</type>
        <comment>Creates an 8-bit QuickTime file (High 4:2:0) with 16-bit AAC audio. The resolution of the file is based on the selected clip. Supported frame rates are 23.98, 24, 25, 29.97, 30, 50, 59.94 and 60 fps.</comment>
        <movie>
                <fileType>QuickTime</fileType>
                <namePattern>&lt;nim_show_root&gt;/FLAME/EDITS/&lt;name&gt;</namePattern>
                <yuvHeadroom>0</yuvHeadroom>
                <yuvColourSpace>PCS_UNKNOWN</yuvColourSpace>
                <colorSyncCompat>False</colorSyncCompat>
        </movie>
        <video>
                <fileType>QuickTime</fileType>
                <codec>33622016</codec>
                <codecProfile>
                        <rootPath>/opt/Autodesk/mediaconverter/</rootPath>
                        <targetVersion>2025</targetVersion>
                        <pathSuffix>/profiles/.33622016/default.cdxprof</pathSuffix>
                </codecProfile>
                <namePattern>&lt;nim_show_root&gt;/FLAME/EDITS/&lt;name&gt;</namePattern>
                <includeAlpha>False</includeAlpha>
                <overwriteWithVersions>False</overwriteWithVersions>
                <resize>
                        <resizeType>fit</resizeType>
                        <resizeFilter>lanczos</resizeFilter>
                        <width>0</width>
                        <height>0</height>
                        <bitsPerChannel>8</bitsPerChannel>
                        <numChannels>3</numChannels>
                        <floatingPoint>False</floatingPoint>
                        <bigEndian>True</bigEndian>
                        <pixelRatio>1</pixelRatio>
                        <scanFormat></scanFormat>
                        <ResSelectorMode>2</ResSelectorMode>
                        <ResAdaptativeMode>0</ResAdaptativeMode>
                        <ResScalingMode>1</ResScalingMode>
                        <ResScalingValue>50</ResScalingValue>
                </resize>
        </video>
        <audio>
                <fileType>QuickTime</fileType>
                <codec>4027060226</codec>
                <codecProfile>
                        <rootPath>/opt/Autodesk/mediaconverter/</rootPath>
                        <targetVersion>2025</targetVersion>
                        <pathSuffix>/profiles/.4027060226/default.cdxprof</pathSuffix>
                </codecProfile>
                <namePattern>&lt;nim_show_root&gt;/FLAME/EDITS/&lt;name&gt;</namePattern>
                <mixdown>To2</mixdown>
                <sampleRate>48000</sampleRate>
                <bitRate>128</bitRate>
                <bitDepth>-1</bitDepth>
                <language>en</language>
                <content></content>
                <channelMapping></channelMapping>
        </audio>
        <subtitle>
                <namePattern>&lt;subtitles name&gt;</namePattern>
                <ReExportColour>False</ReExportColour>
                <ReExportAlignment>1</ReExportAlignment>
                <charFormatting>True</charFormatting>
                <colourMode>NoColour</colourMode>
                <alignmentMode>NoAlignment</alignmentMode>
                <alignment>BottomCentre</alignment>
                <alignmentType>an</alignmentType>
                <timecodeMode>SameAsClip</timecodeMode>
        </subtitle>
</preset>