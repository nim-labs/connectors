<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ExtensionManifest ExtensionBundleId="com.nim-labs.nim" ExtensionBundleName="NIM" ExtensionBundleVersion="1.0" Version="6.0">
  <ExtensionList>
    <Extension Id="com.nim-labs.nim.photoshop" Version="1.0"/>
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="PHXS" Version="16.0"/>
      <Host Name="PHSP" Version="16.0"/>
    </HostList>
    <LocaleList>
      <Locale Code="All"/>
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="6.0"/>
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.nim-labs.nim.photoshop">
      <DispatchInfo>
        <Resources>
          <MainPath>./index.html</MainPath>
        </Resources>
        <UI>
          <Type>Panel</Type>
          <Menu>NIM</Menu>
          <Geometry>
            <Size>
              <Height>280</Height>
              <Width>150</Width>
            </Size>
          </Geometry>
          <Icons>
            <Icon Type="Normal">./img/nim-logo.png</Icon>
            <Icon Type="RollOver">./img/nim-logo-rollover.png</Icon>
            <Icon Type="Disabled">./img/nim-logo-disabled.png</Icon>
            <Icon Type="DarkNormal">./img/nim-logo-dark.png</Icon>
            <Icon Type="DarkRollOver">./img/nim-logo-dark-rollover.png</Icon>
            <Icon Type="DarkDisabled">./img/nim-logo-dark-disabled.png</Icon>
          </Icons>
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>